[
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Anh đấy phải không?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Gì cơ?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Nhà khoa học? Tôi thấy anh trên TV. Tôi tin anh."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Tôi sẽ nói với ông Hammond là ai gọi?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "À, Ian Malcolm. Tôi được mời."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Tim Murphy"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Tiến sĩ Malcolm!"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Trời đất ơi."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Lex Murphy"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Xin chào Tiến sĩ Malcolm."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ôi bọn nhóc."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ["Tim Murphy", "Lex Murphy"]
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Thật tuyệt khi gặp chú."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Gặp các cháu thật tuyệt. Trông cháu đẹp quá."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Tim Murphy"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Chú đến gặp ông nội à?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ừ, ông gọi chú. Cháu biết có chuyện gì không? Chú cũng chẳng biết. Liên doanh này bí hiểm lắm, phải không? Mọi việc ổn cả chứ?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Lex Murphy"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Thật ra thì không."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Quao, Tiến sĩ Malcolm, Tiến sĩ Malcolm. Đến kể chuyện đốt lửa trại với ông bác tôi chắc?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ông có thể thuyết phục tờ Washington Post hoặc tờ Người Điều Tra nếu ông muốn. Nhưng tôi có mặt tại đấy. Tôi biết chuyện gì xản ra. Ông cũng vậy."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ông tin rằng người thận trọng làm điềU đó vì động cơ ác sao? Kể cả Lex và Tim à?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Để họ ra ngoài. Đó không phải trò chơi."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ông đã ký thoả thuận không tiết lộ trước khi tới đảo, nghiêm cấm bàn tán về những gì ông thấy. Ông đã vi phạm thoả thuận."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Phải rồi, còn ông thì nói láo. Ông đã che dấu sự thật về cái chết của 3 người. Ông nhét thông tin sai vào họng công luận. Ông làm tôi như thằng ngu. Ông làm hại sinh kế của tôi."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Chúng tôi đã bồi thường đáng kể cho thiệt hại của ông."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Đó là khoản tiền mất việc và là sự sỉ nhục. Khi ông thêu dệt sự thật, che giấu chứng cớ, nó đau. Nó huỷ hoạt thanh danh của tôi. Nó làm..."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Theo tôi nhớ, trường đại học đã thu hồi bằng của ông vì ông bán câu chuyện về..."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Tôi không bán gì hết, không hề lấy 1 xu, và tôi nói sự thật."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Đó là anh giải thích."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Không có giải thích nào về sự thật cả. Tôi sẽ cho ông biết, Ingen không thể không buồn nôn..."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ingen bây giờ là trách nhiệm của tôi, và tôi sẽ bảo vệ lợi nhuận của nó."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Trách nhiệm của ông? Thế con ông Hammond?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Tôi chịu trách nhiệm với ban giám đốc, chứ không phải với ông bác tôi. Ông phải tin tôi, nhưng vấn đề của ông phải được giải quyết. Trong vài tuần, chúng sẽ rơi vào quên lãng."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Không phải do tôi."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Peter Ludlow"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Cẩn thận. Bộ đồ này đáng giá hơn học vấn của ông đấy."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "John Hammond"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "\"Anh đúng, còn tôi sai.\" Anh chờ để nghe tôi nói điều đó phải không? Cảm ơn Chúa về khu B."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Khu B?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "John Hammond"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Đảo Nublar chỉ là nơi trưng bày thôi, cho du khách ấy mà. Khu B là tầng hầm của nhà máy. Đó là đảo Sorna, cách Nublar 80 dặm. Ở đó chúng tôi nuôi thú, chăm sóc chúng vài tháng rồi chuyển đến Công viên."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Thế à? Tôi chẳng biết gì cả."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "John Hammond"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Giờ, sao tai nạn ở Công viên, cơn bão Clarissa đã quét sạch phương tiện của chúng ta tại khu B. Một hành động của Chúa Trời"
        }
    },
