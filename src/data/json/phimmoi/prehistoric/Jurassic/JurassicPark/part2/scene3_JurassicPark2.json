    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Đi chứ Eddie? Tôi không khoái lắm nhưng vụ này... không từ chối được."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
