    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Bố không phải là người bỏ con ở đây, rồi chuồn đi Paris. Đừng có đổ thừa cho bố."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "<i>Mời Tiến sĩ Malcolm xuống nhà.</i>"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Con gái, bố.... xin lỗi. Con muốn có lời khuyên của bậc cha mẹ, đừng nghe bố. Đừng nghe."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Chúng ta làm gì ở đây?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Specs bảo nó không biến dạng ở... 12,000 P.S.I nên ta sẽ thử."
