    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Eddie Carr"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Nick Van Owen. Đây là Ian Malcolm. Nick là nhiếp ảnh gia của chúng ta."
