    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Nó sẽ hoạt động khi anh quý nó. Để tôi làm cho."
