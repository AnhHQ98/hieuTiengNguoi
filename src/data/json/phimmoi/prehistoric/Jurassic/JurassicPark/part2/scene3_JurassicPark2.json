    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Nào, xong chưa?"
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": ""
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Cái gì đây?"
        }
    },
    {
