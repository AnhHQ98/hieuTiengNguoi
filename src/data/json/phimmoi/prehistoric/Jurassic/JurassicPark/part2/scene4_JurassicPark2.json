    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ờ, gớm thật. Đó luôn là cách nó bắt đầu. Sau đó là chạy và sau đó là la."
        }
    },
    {
        "shotNumber": 1,
        "frame": "",
        "character": {
            "face": "",
            "name": "Sarah Harding"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Ê Nick"
