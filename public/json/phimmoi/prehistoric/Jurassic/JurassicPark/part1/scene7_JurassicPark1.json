    {
        "shotNumber": 1,
        "character": {
            "face": "",
            "name": "Ian Malcolm"
        },
        "subtitle": {
            "engSub": "",
            "vietSub": "Lượng máu làm căng phồng các mạch dẫn, khiếm khuyết trên làn da."
